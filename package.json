{
  "name": "arpanlaha.com",
  "version": "0.1.0",
  "description": "Arpan Laha's personal website",
  "license": "MIT",
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint src --ext .tsx,.ts && markdownlint README.md",
    "lint:fix": "eslint src --ext .tsx,.ts && markdownlint --fix README.md",
    "preload-fonts": "gatsby-preload-fonts",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "typecheck": "npx tsc"
  },
  "dependencies": {
    "gatsby": "^2.20.4",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-manifest": "^2.3.2",
    "gatsby-plugin-offline": "^3.1.1",
    "gatsby-plugin-preload-fonts": "^1.1.1",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-sass": "^2.2.1",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-plugin-split-css": "^1.0.2",
    "gatsby-plugin-typescript": "^2.3.1",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-sharp": "^2.4.2",
    "node-sass": "^4.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@h4iuiuc/eslint-plugin": "^1.1.7",
    "@types/react-helmet": "^5.0.15",
    "eslint": "^6.8.0",
    "markdownlint-cli": "^0.22.0",
    "prettier": "^2.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arpanlaha/arpanlaha.com.git"
  }
}
