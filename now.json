{
  "build": {
    "env": {
      "GATSBY_TELEMETRY_DISABLED": "1"
    }
  },
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/favicon.ico",
      "dest": "/logo.svg"
    },
    {
      "src": "/robots.txt",
      "status": 404
    },
    {
      "src": ".*",
      "status": 308,
      "headers": { "Location": "/page-not-found" }
    }
  ]
}
